<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hiberanate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="com.model.Food" table="food">
        <id name="foodName"/>
        <property name="calories" column="calories" />
        <property name="fats" column="fats" />
        <property name="protein" column="protein" />
        <property name="carbohydrates" column="carbohydrates" />
    </class>
</hibernate-mapping>